<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".kontakdoorsmeer7">

    <TextView
        android:id="@+id/txtwelcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:textSize="30dp"
        android:text="Contact" />

    <TextView
        android:id="@+id/txtwelcome1"
        android:layout_below="@+id/txtwelcome"
        android:layout_width="220dp"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:textSize="15dp"
        android:text="Untuk pemesanan lebih lanjut silahkan hubungi contact dibawah" />

    <ImageView
        android:id="@+id/gambar1"
        android:layout_below="@+id/txtwelcome1"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="70dp"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/whatsapp"/>

    <TextView
        android:layout_below="@+id/txtwelcome1"
        android:layout_marginTop="110dp"
        android:layout_marginLeft="150dp"
        android:text="085675458762"
        android:textSize="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <ImageView
        android:id="@+id/gambar2"
        android:layout_below="@+id/txtwelcome1"
        android:layout_marginTop="210dp"
        android:layout_marginLeft="70dp"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/phone"/>

    <TextView
        android:layout_below="@+id/txtwelcome1"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="150dp"
        android:text="089672563273"
        android:textSize="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btnkembali"
        android:layout_below="@+id/txtwelcome1"
        android:layout_marginTop="380dp"
        android:background="@drawable/custom_button"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="220dp"
        android:text="Kembali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</RelativeLayout>